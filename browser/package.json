{
  "name": "@kenrick95/c4-browser",
  "version": "1.0.0",
  "description": "Connect Four game in browser",
  "repository": {
    "type": "git",
    "url": "https://github.com/kenrick95/c4.git"
  },
  "author": {
    "name": "Kenrick",
    "email": "kenrick95@gmail.com",
    "url": "https://kenrick95.org"
  },
  "bugs": {
    "url": "https://github.com/kenrick95/c4/issues"
  },
  "homepage": "https://github.com/kenrick95/c4",
  "license": "MIT",
  "keywords": [
    "c4",
    "connect-four",
    "game"
  ],
  "source": "./src/app.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "start": "parcel serve --no-cache src/index.html",
    "build": "yarn clean && cross-env parcel build --no-cache src/index.html",
    "build-gh-pages": "yarn clean && parcel build src/index.html --public-url \"https://kenrick95.github.io/c4/dist/\"",
    "pretty": "prettier --write \"./**/*.?(ts|html|scss|css)\"",
    "clean": "shx rm -rf dist/"
  },
  "dependencies": {
    "@kenrick95/c4": "4.0.0",
    "core-js": "^3.16.0",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@types/node": "^16.4.13",
    "cross-env": "^7.0.3",
    "parcel": "nightly",
    "prettier": "^2.3.2",
    "shx": "^0.3.3",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "minimist": "^1.2.2"
  }
}
